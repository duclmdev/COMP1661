<?xml version='1.0' encoding='utf-8'?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" id="edu.fpt.comp1661.storage" version="1.0.0"
        xmlns="http://www.w3.org/ns/widgets">
    <name>My Storage</name>
    <description>My Storage - Project for COMP1661 course</description>
    <author email="duclmdev@gmail.com" href="http://www.github.com/duclmdev">Le Minh Duc</author>
    <content src="index.html"/>
    <plugin name="cordova-plugin-whitelist" spec="1"/>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    </platform>
    <plugin name="storage-plugin" spec="storage-plugin"/>
    <engine name="android" spec="^7.1.4"/>
    <engine name="browser" spec="^5.0.4"/>
</widget>
